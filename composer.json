{
    "name": "achillesp/laravel-filterable",
    "description": "Filter eloquent models in Laravel.",
    "license": "MIT",
    "keywords": ["laravel", "eloquent", "filter"],
    "authors": [
        {
            "name": "Achilles Panagiotou",
            "email": "achilles.p@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
